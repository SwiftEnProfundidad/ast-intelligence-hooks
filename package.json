{
  "name": "pumuki-ast-hooks",
  "version": "6.3.5",
  "description": "Enterprise-grade AST Intelligence System with multi-platform support (iOS, Android, Backend, Frontend) and Feature-First + DDD + Clean Architecture enforcement. Includes dynamic violations API for intelligent querying.",
  "main": "index.js",
  "bin": {
    "pumuki-ast-hooks": "bin/pumuki-framework.js",
    "ast-hooks": "bin/pumuki-framework.js",
    "pumuki-framework": "bin/pumuki-framework.js",
    "pumuki-pre-commit": "bin/pumuki-pre-commit.js",
    "pumuki-pre-push": "bin/pumuki-pre-push.js",
    "pumuki-ci": "bin/pumuki-ci.js",
    "pumuki-mcp-evidence": "bin/pumuki-mcp-evidence.js"
  },
  "scripts": {
    "install-hooks": "node bin/install.js",
    "check-version": "node bin/check-version.js",
    "audit": "node bin/ast",
    "ast": "node bin/ast",
    "audit-library": "node bin/audit-library.js",
    "gitflow": "bash bin/gitflow check",
    "gitflow:status": "bash bin/gitflow status",
    "gitflow:workflow": "bash bin/gitflow workflow",
    "gitflow:reset": "bash bin/gitflow reset",
    "ast:gitflow": "node scripts/hooks-system/bin/gitflow-cycle.js",
    "ast:release": "node scripts/hooks-system/bin/gitflow-release.js",
    "violations": "node bin/violations",
    "violations:list": "node bin/violations list",
    "violations:show": "node bin/violations show",
    "violations:summary": "node bin/violations summary",
    "violations:top": "node bin/violations top",
    "violations:demo": "bash bin/demo-violations",
    "test": "jest --runInBand",
    "lint": "npm run lint:hooks",
    "lint:hooks": "eslint \"legacy/scripts/hooks-system/application/**/*.js\" \"legacy/scripts/hooks-system/bin/**/*.js\" \"legacy/bin/**/*.js\"",
    "build:ts": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test:heuristics": "npx --yes tsx@4.21.0 --test core/facts/__tests__/extractHeuristicFacts.test.ts",
    "test:evidence": "npx --yes tsx@4.21.0 --test integrations/evidence/__tests__/buildEvidence.test.ts integrations/evidence/__tests__/humanIntent.test.ts",
    "test:mcp": "npx --yes tsx@4.21.0 --test integrations/mcp/__tests__/evidenceContextServer.test.ts",
    "test:stage-gates": "npx --yes tsx@4.21.0 --test integrations/config/__tests__/*.test.ts integrations/gate/__tests__/*.test.ts integrations/git/__tests__/*.test.ts scripts/__tests__/*.test.ts",
    "test:deterministic": "npm run test:evidence && npm run test:mcp && npm run test:heuristics",
    "ast:refresh": "bash scripts/hooks-system/bin/update-evidence.sh",
    "ast:audit": "node scripts/hooks-system/infrastructure/ast/ast-intelligence.js",
    "ast:guard:start": "bash scripts/hooks-system/bin/evidence-guard start",
    "ast:guard:stop": "bash scripts/hooks-system/bin/evidence-guard stop",
    "ast:guard:restart": "bash scripts/hooks-system/bin/evidence-guard restart",
    "ast:guard:status": "bash scripts/hooks-system/bin/evidence-guard status",
    "ast:guard:logs": "bash scripts/hooks-system/bin/evidence-guard logs",
    "ast:check-version": "node scripts/hooks-system/bin/check-version.js",
    "framework:menu": "node bin/pumuki-framework.js",
    "validation:consumer-ci-artifacts": "npx --yes tsx@4.21.0 scripts/collect-consumer-ci-artifacts.ts",
    "validation:consumer-ci-auth-check": "npx --yes tsx@4.21.0 scripts/check-consumer-ci-auth.ts",
    "validation:consumer-workflow-lint": "npx --yes tsx@4.21.0 scripts/lint-consumer-workflows.ts",
    "validation:consumer-support-bundle": "npx --yes tsx@4.21.0 scripts/build-consumer-startup-failure-support-bundle.ts",
    "validation:consumer-support-ticket-draft": "npx --yes tsx@4.21.0 scripts/build-consumer-support-ticket-draft.ts",
    "validation:consumer-startup-unblock-status": "npx --yes tsx@4.21.0 scripts/build-consumer-startup-unblock-status.ts",
    "validation:consumer-startup-triage": "npx --yes tsx@4.21.0 scripts/build-consumer-startup-triage.ts",
    "validation:mock-consumer-ab-report": "npx --yes tsx@4.21.0 scripts/build-mock-consumer-ab-report.ts",
    "validation:adapter-readiness": "npx --yes tsx@4.21.0 scripts/build-adapter-readiness.ts",
    "validation:adapter-real-session-report": "npx --yes tsx@4.21.0 scripts/build-adapter-real-session-report.ts",
    "validation:adapter-session-status": "npx --yes tsx@4.21.0 scripts/build-adapter-session-status.ts",
    "validation:phase5-blockers-readiness": "npx --yes tsx@4.21.0 scripts/build-phase5-blockers-readiness.ts",
    "validation:phase5-execution-closure-status": "npx --yes tsx@4.21.0 scripts/build-phase5-execution-closure-status.ts",
    "validation:phase5-execution-closure": "npx --yes tsx@4.21.0 scripts/run-phase5-execution-closure.ts",
    "validation:phase5-external-handoff": "npx --yes tsx@4.21.0 scripts/build-phase5-external-handoff.ts",
    "validation:package-manifest": "node --import tsx scripts/check-package-manifest.ts",
    "validation:package-smoke": "node --import tsx scripts/package-install-smoke.ts --mode=block",
    "validation:package-smoke:minimal": "node --import tsx scripts/package-install-smoke.ts --mode=minimal",
    "validation:clean-artifacts": "npx --yes tsx@4.21.0 scripts/clean-validation-artifacts.ts",
    "validation:docs-hygiene": "npx --yes tsx@4.21.0 scripts/check-validation-docs-hygiene.ts",
    "skills:compile": "npx --yes tsx@4.21.0 scripts/compile-skills-lock.ts",
    "skills:lock:check": "npx --yes tsx@4.21.0 scripts/compile-skills-lock.ts --check",
    "mcp:evidence": "node bin/pumuki-mcp-evidence.js",
    "validate:adapter-hooks-local": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/validate-local-runtime.sh",
    "print:adapter-hooks-config": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/print-adapter-hooks-config.sh",
    "install:adapter-hooks-config": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/install-adapter-hooks-config.sh",
    "verify:adapter-hooks-runtime": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/verify-adapter-hooks-runtime.sh",
    "assess:adapter-hooks-session": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/assess-adapter-session.sh",
    "assess:adapter-hooks-session:any": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/assess-adapter-session.sh --include-simulated"
  },
  "keywords": [
    "ast",
    "code-quality",
    "static-analysis",
    "ios",
    "android",
    "backend",
    "frontend",
    "clean-architecture",
    "ddd",
    "feature-first",
    "git-hooks",
    "pre-commit",
    "typescript",
    "swift",
    "kotlin",
    "react",
    "nextjs",
    "nestjs"
  ],
  "author": {
    "name": "Juan Carlos Merlos AlbarracÃ­n",
    "email": "freelancemerlos@gmail.com",
    "organization": "Pumuki Team"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SwiftEnProfundidad/ast-intelligence-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/SwiftEnProfundidad/ast-intelligence-hooks/issues"
  },
  "homepage": "https://github.com/SwiftEnProfundidad/ast-intelligence-hooks#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "glob": "^10.5.0",
    "ts-morph": ">=21.0.0",
    "tsx": "^4.21.0",
    "@babel/parser": "^7.28.5"
  },
  "peerDependencies": {
    "ts-morph": ">=21.0.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@types/node": "^20.10.0",
    "eslint": "^9.12.0",
    "jest": "^30.2.0",
    "jscodeshift": "^17.3.0",
    "recast": "^0.23.11",
    "typescript": "^5.3.0"
  },
  "files": [
    "bin/*.js",
    "core/facts/*.ts",
    "core/gate/*.ts",
    "core/rules/*.ts",
    "core/rules/presets/*.ts",
    "integrations/config/*.ts",
    "integrations/evidence/*.ts",
    "integrations/gate/*.ts",
    "integrations/git/*.ts",
    "integrations/mcp/*.ts",
    "integrations/platform/*.ts",
    "scripts/*.ts",
    "docs/*.md",
    "docs/rule-packs/*.md",
    "docs/validation/*.md",
    "docs/images/README.md",
    "assets/*",
    "index.js",
    "README.md",
    "LICENSE",
    "VERSION",
    "tsconfig.json",
    "skills.lock.json",
    "skills.sources.json"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json",
    "./integrations/git": "./integrations/git/index.ts",
    "./integrations/mcp": "./integrations/mcp/index.ts",
    "./core/gate/evaluateGate": "./core/gate/evaluateGate.ts",
    "./core/gate/evaluateRules": "./core/gate/evaluateRules.ts"
  }
}
