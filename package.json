{
  "name": "pumuki-ast-hooks",
  "version": "6.3.5",
  "description": "Enterprise-grade AST Intelligence System with multi-platform support (iOS, Android, Backend, Frontend) and Feature-First + DDD + Clean Architecture enforcement. Includes dynamic violations API for intelligent querying.",
  "main": "index.js",
  "bin": {
    "pumuki-ast-hooks": "bin/pumuki-mcp-server.js",
    "audit": "bin/audit",
    "ast-hooks": "bin/cli.js",
    "ast-install": "bin/install.js",
    "ast-uninstall": "bin/uninstall.js",
    "ast-violations": "bin/violations-api.js",
    "ast-check-version": "bin/check-version.js",
    "ast-gitflow": "scripts/hooks-system/bin/gitflow-cycle.js",
    "ai-commit": "bin/ai-commit.sh",
    "hook-run-orchestrator": "bin/run-orchestrator.js",
    "hook-watch": "bin/watch-hooks.js",
    "hook-status": "bin/hook-status.js",
    "hook-predict": "bin/predictive-hooks.js",
    "hook-playbook": "bin/run-playbook.js",
    "hook-doc-drift": "bin/check-doc-drift.js",
    "hook-plan-review": "bin/plan-review.js"
  },
  "scripts": {
    "install-hooks": "node bin/install.js",
    "check-version": "node bin/check-version.js",
    "audit": "node bin/ast",
    "ast": "node bin/ast",
    "audit-library": "node bin/audit-library.js",
    "gitflow": "bash bin/gitflow check",
    "gitflow:status": "bash bin/gitflow status",
    "gitflow:workflow": "bash bin/gitflow workflow",
    "gitflow:reset": "bash bin/gitflow reset",
    "ast:gitflow": "node scripts/hooks-system/bin/gitflow-cycle.js",
    "ast:release": "node scripts/hooks-system/bin/gitflow-release.js",
    "violations": "node bin/violations",
    "violations:list": "node bin/violations list",
    "violations:show": "node bin/violations show",
    "violations:summary": "node bin/violations summary",
    "violations:top": "node bin/violations top",
    "violations:demo": "bash bin/demo-violations",
    "test": "jest --runInBand",
    "lint": "npm run lint:hooks",
    "lint:hooks": "eslint \"scripts/hooks-system/application/**/*.js\" \"bin/**/*.js\"",
    "build:ts": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test:heuristics": "npx --yes tsx@4.21.0 --test core/facts/__tests__/extractHeuristicFacts.test.ts",
    "test:evidence": "npx --yes tsx@4.21.0 --test integrations/evidence/__tests__/buildEvidence.test.ts integrations/evidence/__tests__/humanIntent.test.ts",
    "test:mcp": "npx --yes tsx@4.21.0 --test integrations/mcp/__tests__/evidenceContextServer.test.ts",
    "test:stage-gates": "npx --yes tsx@4.21.0 --test integrations/config/__tests__/skillsRuleSet.test.ts integrations/config/__tests__/skillsContracts.test.ts integrations/config/__tests__/compileSkillsLock.test.ts integrations/gate/__tests__/stagePolicies.test.ts integrations/git/__tests__/resolveGitRefs.test.ts integrations/git/__tests__/stageRunners.test.ts scripts/__tests__/framework-menu.test.ts",
    "test:deterministic": "npm run test:evidence && npm run test:mcp && npm run test:heuristics",
    "ast:refresh": "bash scripts/hooks-system/bin/update-evidence.sh",
    "ast:audit": "node scripts/hooks-system/infrastructure/ast/ast-intelligence.js",
    "ast:guard:start": "bash scripts/hooks-system/bin/evidence-guard start",
    "ast:guard:stop": "bash scripts/hooks-system/bin/evidence-guard stop",
    "ast:guard:restart": "bash scripts/hooks-system/bin/evidence-guard restart",
    "ast:guard:status": "bash scripts/hooks-system/bin/evidence-guard status",
    "ast:guard:logs": "bash scripts/hooks-system/bin/evidence-guard logs",
    "ast:check-version": "node scripts/hooks-system/bin/check-version.js",
    "framework:menu": "npx tsx scripts/framework-menu.ts",
    "validation:consumer-ci-artifacts": "npx --yes tsx@4.21.0 scripts/collect-consumer-ci-artifacts.ts",
    "skills:compile": "npx --yes tsx@4.21.0 scripts/compile-skills-lock.ts",
    "skills:lock:check": "npx --yes tsx@4.21.0 scripts/compile-skills-lock.ts --check",
    "mcp:evidence": "npx --yes tsx@4.21.0 integrations/mcp/evidenceContextServer.cli.ts",
    "validate:windsurf-hooks-local": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/validate-local-runtime.sh",
    "print:windsurf-hooks-config": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/print-windsurf-hooks-config.sh",
    "install:windsurf-hooks-config": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/install-windsurf-hooks-config.sh",
    "verify:windsurf-hooks-runtime": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/verify-windsurf-hooks-runtime.sh",
    "assess:windsurf-hooks-session": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/assess-windsurf-session.sh",
    "assess:windsurf-hooks-session:any": "bash legacy/scripts/hooks-system/infrastructure/cascade-hooks/assess-windsurf-session.sh --include-simulated"
  },
  "keywords": [
    "ast",
    "code-quality",
    "static-analysis",
    "ios",
    "android",
    "backend",
    "frontend",
    "clean-architecture",
    "ddd",
    "feature-first",
    "git-hooks",
    "pre-commit",
    "typescript",
    "swift",
    "kotlin",
    "react",
    "nextjs",
    "nestjs"
  ],
  "author": {
    "name": "Juan Carlos Merlos AlbarracÃ­n",
    "email": "freelancemerlos@gmail.com",
    "organization": "Pumuki Team"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SwiftEnProfundidad/ast-intelligence-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/SwiftEnProfundidad/ast-intelligence-hooks/issues"
  },
  "homepage": "https://github.com/SwiftEnProfundidad/ast-intelligence-hooks#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "glob": "^10.5.0",
    "ts-morph": ">=21.0.0"
  },
  "peerDependencies": {
    "ts-morph": ">=21.0.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@types/node": "^20.10.0",
    "eslint": "^9.12.0",
    "jest": "^30.2.0",
    "jscodeshift": "^17.3.0",
    "recast": "^0.23.11",
    "typescript": "^5.3.0"
  },
  "files": [
    "bin/",
    "scripts/hooks-system/",
    "scripts/hooks-system/config/",
    "presentation/",
    "skills/",
    "hooks/",
    "docs/",
    "assets/",
    "index.js",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json",
    "./ast": "./scripts/hooks-system/infrastructure/ast/ast-intelligence.js",
    "./domain": "./scripts/hooks-system/domain/index.js",
    "./application": "./scripts/hooks-system/application/index.js",
    "./config": "./config/index.js",
    "./skills": "./skills/skill-rules.json",
    "./hooks": "./hooks/index.js"
  }
}
