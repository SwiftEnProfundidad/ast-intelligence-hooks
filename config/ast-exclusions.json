{
    "version": "1.0.0",
    "description": "AST rule exclusions ONLY for third-party code and build artifacts",
    "exclusions": {
        "paths": {
            "node_modules": true,
            "dist": true,
            ".next": true,
            "coverage": true,
            ".audit_tmp": true,
            ".ast-intelligence/tsc-cache": true,
            ".ast-intelligence/hooks": true,
            "build": true,
            ".build": true,
            "checkouts": true,
            "Pods": true,
            "DerivedData": true
        },
        "rules": {
            "ios.force_unwrapping": {
                "reason": ""
            }
        }
    },
    "severityOverrides": {
        "common.docs.missing_readme": "LOW"
    }
}